function ohje(otsikko,teksti) {
	if (teksti == '') {
		otsikko = 'Ohje';
		teksti = 'Anna hakukriteerit ja paina \'Hae\'';
	}
	ohjeteksti = '<i>'+otsikko+'</i><br/><br/>'+teksti;
	document.getElementById("help").innerHTML=ohjeteksti;
	return;
}

function focus_laji() {
      	document.getElementById("paaformi").laji.focus();
}

function dis_ena_negahaku(arvo) {
	/**	20131206 AJL Jos lajikentässä on jotakin, näytetään negahakun kruxi-kenttä
	*	20160116 AJL Jos ollaan peruslomakkeella, negahaku-kentää ei ole, ohitetaan.
	*/
	var piilo = '';

	if (typeof arvo.value == 'undefined' || arvo == null) {
		piilo = '';
	} else {
		piilo = arvo.value;
	}

	// 20160127 AJL
	var loytyyko = document.getElementById("negahakubox");
	if (loytyyko == null) {
		; // ei ollut, ohitetaan
	} else {
		if (piilo=='') {
			document.getElementById("negahakubox").style.display = "none";
		} else {
			/* document.getElementById("negahakubox").style.display = "block"; */
			document.getElementById("negahakubox").style.display = "";
		}
	}
}

