function kerutoimpiilo() {
	v=document.getElementById("aluevalikko").value;
        if (v==2 || v==3 || v==4 || v==10 || v==11 || v==13) { // näytetään kerutoim TLY, Tringa, Porvoo, PiLY, PLY, MLY
